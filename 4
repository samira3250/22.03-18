function checkAnniversary(birthDateString) {
  const birthDate = new Date(birthDateString);
  const birthYear = birthDate.getFullYear();
  const birthMonth = birthDate.getMonth();
  const birthDay = birthDate.getDate();
  const now = new Date();
  const currentYear = now.getFullYear();
  const age = currentYear - birthYear;
  if (age < 0) {
    return "Дата рождения в будущем!";
  }
  const anniversaryYear = age % 10 === 0; 
  const birthdayThisYear = new Date(currentYear, birthMonth, birthDay);
  const birthdayThisYearTimestamp = birthdayThisYear.valueOf(); 
  const nowTimestamp = now.valueOf();
  if (anniversaryYear) {
    if (birthdayThisYearTimestamp <= nowTimestamp) {
      return "В этом году уже был юбилей!";
    } else {
      return "В этом году будет юбилей!";
    }
  } else {
    return "В этом году нет юбилея.";
  }
}
const birthDate = "2007-05-24"; 
const anniversaryMessage = checkAnniversary(birthDate);
console.log(anniversaryMessage);
